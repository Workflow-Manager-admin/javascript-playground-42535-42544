{
    "name": "javascript-playground-backend",
    "version": "1.0.0",
    "description": "Backend API for JavaScript playground application with secure code execution, authentication, and snippet management",
    "main": "src/server.js",
    "scripts": {
        "start": "node src/server.js",
        "dev": "nodemon src/server.js",
        "test": "jest",
        "lint": "eslint .",
        "generate-openapi": "node generate_openapi.js"
    },
    "dependencies": {
        "bcryptjs": "^3.0.2",
        "cors": "2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "isolated-vm": "^5.0.4",
        "jsonwebtoken": "^9.0.2",
        "mysql2": "^3.14.3",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "eslint": "^9.24.0",
        "globals": "^16.0.0",
        "jest": "^29.7.0",
        "nodemon": "^3.0.1"
    },
    "nodemonConfig": {
        "watch": [
            "src"
        ],
        "ignore": [
            "node_modules",
            "package.json",
            "package-lock.json",
            "*.lock",
            "*.log",
            "*.md",
            ".git",
            "dist",
            "build"
        ],
        "ext": "js,json"
    },
    "keywords": [
        "javascript",
        "playground",
        "code-execution",
        "express",
        "api",
        "jwt",
        "mysql"
    ],
    "author": "JavaScript Playground Team",
    "license": "MIT"
}
